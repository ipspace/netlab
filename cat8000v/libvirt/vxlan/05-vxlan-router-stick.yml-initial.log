    if [ ! -e /sys/devices/virtual/net/vlan1001 ]; then
    ip link add vlan1001 type bridge
    ip link set up dev vlan1001
    fi
    ip link set dev vxlan5001 master vlan1001
    ip link set vlan1001 type bridge stp_state 0
  
