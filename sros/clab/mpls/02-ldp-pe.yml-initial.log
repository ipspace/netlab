      val:
       admin-state: disable
  
    - path: configure/router[router-name=Base]/ldp
      val:
       admin-state: enable
       implicit-null-label: True
       interface-parameters:
        interface:
        - ip-int-name: "eth-1/1/c2"
          admin-state: enable
          ipv4:
           admin-state: enable
           transport-address: system
  
  
    - path: configure/router[router-name=Base]/bgp/next-hop-resolution
