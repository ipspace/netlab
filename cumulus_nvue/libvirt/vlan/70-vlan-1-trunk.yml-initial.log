    #
    10.0.0.1 s1
    172.31.1.1 vlan1.s1
    10.0.0.2 s2
    172.31.1.2 vlan1.s2
    10.0.0.3 s3
    172.31.1.3 vlan1.s3
    10.0.0.4 s4
