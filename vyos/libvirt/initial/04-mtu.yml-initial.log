    #!/bin/vbash
    source /opt/vyatta/etc/functions/script-template
  
    if [ "$(id -g -n)" != 'vyattacfg' ] ; then
        exec sg vyattacfg -c "/bin/vbash $(readlink -f $0) $@"
    fi
  
    # Configuration items start here
  
    configure
    set system host-name 'vyos-r'
  
  
    set interfaces dummy dum0 address 10.0.0.1/32
    set interfaces dummy dum0 address 2001:db8:0:1::1/64
  
  
  
    set interfaces ethernet eth1 description 'r -> h1 [stub]'
  
    set interfaces ethernet eth1 mtu 1400
  
    set interfaces ethernet eth1 address 172.16.0.1/24
  
    set interfaces ethernet eth1 address 2001:db8:1::1/64;
  
  
  
  
    set interfaces ethernet eth2 description 'r -> h2 [stub]'
  
    set interfaces ethernet eth2 mtu 1600
  
    set interfaces ethernet eth2 address 172.16.1.1/24
  
    set interfaces ethernet eth2 address 2001:db8:1:1::1/64;
  
  
  
    set service lldp interface all
    set service lldp interface eth0 disable
  
    set service router-advert interface eth1
    set service router-advert interface eth2
  
  
    # Commit, save and exit from subshell
  
    commit
    save
    exit
  
    # Restart FRR to pick up the new hostname
    sudo service frr restart

