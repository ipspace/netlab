    #!/bin/vbash
    source /opt/vyatta/etc/functions/script-template
  
    if [ "$(id -g -n)" != 'vyattacfg' ] ; then
        exec sg vyattacfg -c "/bin/vbash $(readlink -f $0) $@"
    fi
  
    # Configuration items start here
  
    configure
    set system host-name 's2'
  
  
    set interfaces bridge br0 description 'Global Switch Bridge'
    set interfaces bridge br0 enable-vlan
  
    set interfaces bridge br0 vif 701
    set interfaces bridge br0 vif 700
  
    set interfaces dummy dum0 address 10.0.0.4/32
  
  
  
    set interfaces ethernet eth1 description 's2 -> s1'
  
  
  
  
  
  
  
    set interfaces ethernet eth2 description '[Access VLAN red] s2 -> r1'
  
  
  
  
  
  
  
    set interfaces ethernet eth3 description '[Access VLAN blue] s2 -> r1'
  
  
  
  
  
  
  
    set interfaces bridge br0 vif 700 description 'VLAN red (700) -> [h1,s1,r1]'
  
  
  
  
  
  
  
    set interfaces bridge br0 vif 701 description 'VLAN blue (701) -> [h2,s1,r1]'
  
  
  
  
  
  
    set service lldp interface all
    set service lldp interface eth0 disable
  
  
  
    # Commit, save and exit from subshell
  
    commit
    save
    exit
  
    # Restart FRR to pick up the new hostname
    sudo service frr restart
