    #!/bin/vbash
    source /opt/vyatta/etc/functions/script-template
  
    if [ "$(id -g -n)" != 'vyattacfg' ] ; then
        exec sg vyattacfg -c "/bin/vbash $(readlink -f $0) $@"
    fi
  
    # Configuration items start here
  
    configure
    set system host-name 's1'
  
  
    set interfaces bridge br0 description 'Global Switch Bridge'
    set interfaces bridge br0 enable-vlan
  
    set interfaces bridge br0 vif 700
  
    set interfaces dummy dum0 address 10.0.0.4/32
  
  
  
    set interfaces ethernet eth1 description 's1 -> h3 [stub]'
  
  
    set interfaces ethernet eth1 address 172.16.1.4/24
  
  
  
  
  
    set interfaces ethernet eth2 description '[Access VLAN red] s1 -> h1'
  
  
  
  
  
  
  
    set interfaces ethernet eth3 description '[Access VLAN red] s1 -> h2'
  
  
  
  
  
  
  
    set interfaces bridge br0 vif 700 description 'VLAN red (700) -> [h1,h2] [stub]'
  
  
  
  
  
  
    set service lldp interface all
    >/dev/null 2>/dev/null set service lldp interface eth0 disable
    >/dev/null 2>/dev/null set service lldp interface eth0 mode disable
  
  
  
    # Commit, save and exit from subshell
  
    commit
    save
    exit
  
