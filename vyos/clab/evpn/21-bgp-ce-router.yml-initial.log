    #!/bin/vbash
    source /opt/vyatta/etc/functions/script-template
  
    if [ "$(id -g -n)" != 'vyattacfg' ] ; then
        exec sg vyattacfg -c "/bin/vbash $(readlink -f $0) $@"
    fi
  
    # Configuration items start here
  
    configure
    set system host-name 's1'
  
    set vrf name tenant table 100
  
    set interfaces bridge br0 description 'Global Switch Bridge'
    set interfaces bridge br0 enable-vlan
  
    set interfaces bridge br0 vif 1001
    set interfaces bridge br0 vif 1000
  
    set interfaces dummy dum0 address 10.0.0.3/32
  
  
  
    set interfaces ethernet eth1 description 's1 -> s2'
  
    set interfaces ethernet eth1 mtu 1600
  
    set interfaces ethernet eth1 address 10.1.0.1/30
  
  
  
  
  
    set interfaces ethernet eth2 description 's1 -> ce1 [external]'
  
    set interfaces ethernet eth2 mtu 1500
  
    set interfaces ethernet eth2 address 10.1.0.6/30
  
  
    set interfaces ethernet eth2 vrf tenant
  
  
  
    set interfaces bridge br0 vif 1000 description 'VLAN red (1000) -> [s2]'
  
  
    set interfaces bridge br0 vif 1000 address 172.16.0.3/24
  
  
  
  
  
    set interfaces bridge br0 vif 1001 description 'VLAN blue (1001) -> [s2]'
  
  
    set interfaces bridge br0 vif 1001 address 172.16.1.3/24
  
  
  
  
