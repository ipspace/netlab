    #!/bin/vbash
    source /opt/vyatta/etc/functions/script-template
  
    if [ "$(id -g -n)" != 'vyattacfg' ] ; then
        exec sg vyattacfg -c "/bin/vbash $(readlink -f $0) $@"
    fi
  
    # Configuration items start here
  
    configure
    set system host-name 'spine'
  
  
    set interfaces dummy dum0 address 10.0.0.1/32
  
  
  
    set interfaces ethernet eth1 description 'spine -> l1 [external]'
  
    set interfaces ethernet eth1 mtu 1600
  
    set interfaces ethernet eth1 address 10.1.0.2/30
  
  
  
  
  
    set interfaces ethernet eth2 description 'spine -> l2 [external]'
  
    set interfaces ethernet eth2 mtu 1600
  
    set interfaces ethernet eth2 address 10.1.0.6/30
  
  
  
  
  
    set interfaces ethernet eth3 description 'spine -> l3'
  
    set interfaces ethernet eth3 mtu 1600
  
    set interfaces ethernet eth3 address 10.1.0.10/30
  
  
  
  
    set service lldp interface all
    >/dev/null 2>/dev/null set service lldp interface eth0 disable
    >/dev/null 2>/dev/null set service lldp interface eth0 mode disable
  
  
    set service ssh
  
    # Commit, save and exit from subshell
  
    commit
    save
    exit
  
    # Restart FRR to pick up the new hostname
    sudo service frr restart
