    #!/bin/vbash
    source /opt/vyatta/etc/functions/script-template
  
    if [ "$(id -g -n)" != 'vyattacfg' ] ; then
        exec sg vyattacfg -c "/bin/vbash $(readlink -f $0) $@"
    fi
  
    # Configuration items start here
  
    configure
    set system host-name 'rtr'
  
    set vrf name blue table 101
    set vrf name red table 100
  
    set interfaces dummy dum0 address 10.0.0.5/32
    set interfaces dummy dum0 address 2001:db8:1:5::1/64
  
  
  
    set interfaces ethernet eth1 description 'rtr -> h1 [stub]'
  
    set interfaces ethernet eth1 mtu 1500
  
  
    set interfaces ethernet eth1 address 2001:db8:2::5/64;
  
    set interfaces ethernet eth1 vrf red
  
  
  
    set interfaces ethernet eth2 description 'rtr -> h3 [stub]'
  
    set interfaces ethernet eth2 mtu 1500
  
  
    set interfaces ethernet eth2 address 2001:db8:2:1::5/64;
  
    set interfaces ethernet eth2 vrf red
  
  
  
    set interfaces ethernet eth3 description 'rtr -> h2 [stub]'
  
    set interfaces ethernet eth3 mtu 1500
  
  
    set interfaces ethernet eth3 address 2001:db8:2:2::5/64;
  
    set interfaces ethernet eth3 vrf blue
  
  
  
    set interfaces ethernet eth4 description 'rtr -> h4 [stub]'
  
    set interfaces ethernet eth4 mtu 1500
  
  
    set interfaces ethernet eth4 address 2001:db8:2:3::5/64;
  
    set interfaces ethernet eth4 vrf blue
  
  
    set service lldp interface all
    >/dev/null 2>/dev/null set service lldp interface eth0 disable
    >/dev/null 2>/dev/null set service lldp interface eth0 mode disable
  
    set service router-advert interface eth1
    set service router-advert interface eth2
    set service router-advert interface eth3
    set service router-advert interface eth4
  
    set service ssh
  
    # Commit, save and exit from subshell
  
    commit
    save
    exit
  
    # Restart FRR to pick up the new hostname
    sudo service frr restart

