    #!/bin/vbash
    source /opt/vyatta/etc/functions/script-template
  
    if [ "$(id -g -n)" != 'vyattacfg' ] ; then
        exec sg vyattacfg -c "/bin/vbash $(readlink -f $0) $@"
    fi
  
    # Configuration items start here
  
    configure
    set system host-name 'ros'
  
  
    set interfaces bridge br0 description 'Global Switch Bridge'
    set interfaces bridge br0 enable-vlan
  
  
    set interfaces dummy dum0 address 10.0.0.1/32
  
  
  
    set interfaces ethernet eth1 description 'ros -> s2'
  
    set interfaces ethernet eth1 mtu 1500
  
  
  
  
  
  
    set interfaces ethernet eth1 vif 701 description 'ros -> [r2,s1,s2]'
  
  
    set interfaces ethernet eth1 vif 701 address 172.16.1.1/24
  
  
  
  
  
    set interfaces ethernet eth1 vif 700 description 'ros -> [r1,s1,s2]'
  
  
    set interfaces ethernet eth1 vif 700 address 172.16.0.1/24
  
  
  
  
    set service lldp interface all
    >/dev/null 2>/dev/null set service lldp interface eth0 disable
    >/dev/null 2>/dev/null set service lldp interface eth0 mode disable
  
  
    set service ssh
  
    # Commit, save and exit from subshell
  
    commit
    save
    exit
  
    # Restart FRR to pick up the new hostname
    sudo service frr restart
