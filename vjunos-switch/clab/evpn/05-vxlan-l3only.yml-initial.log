            community add tg_65000_1;
            accept;
          }
        }
      }
  
  
  
      policy-statement vrf-customer-import {
        term 1 {
          from community [ tg_65000_1 ];
          then accept;
        }
        term default {
          then reject;
        }
      }
    }
  
    routing-instances {
  
      customer {
        instance-type vrf;
        route-distinguisher 65000:1;
  
        vrf-import vrf-customer-import;
        vrf-export vrf-customer-export;
  
        routing-options {
          auto-export;
        }
  
        interface ge-0/0/0.0;
  
