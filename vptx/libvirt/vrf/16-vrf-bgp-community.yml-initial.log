    system {
      host-name dut
        static-host-mapping {
            x1 inet 10.0.0.10;
            x2 inet 10.0.0.11;
        }
    }
  
  
  
  
  
  
    policy-options {
      community tg_65000_1 members target:65000:1;
    }
  
  
  
    policy-options {
      policy-statement vrf-red-export {
        term 1 {
          then {
            community add tg_65000_1;
            accept;
          }
        }
      }
  
  
  
      policy-statement vrf-red-import {
        term 1 {
          from community [ tg_65000_1 ];
          then accept;
        }
        term default {
          then reject;
        }
      }
    }
  
    routing-instances {
  
      red {
        instance-type vrf;
        route-distinguisher 65000:1;
  
        vrf-import vrf-red-import;
        vrf-export vrf-red-export;
  
        routing-options {
          auto-export;
        }
  
        interface et-0/0/0.0;
        interface et-0/0/1.0;
  
      }
  
    }
    interfaces {
  
      lo0.0 {
  
          family inet {
            address 10.0.0.1/32;
          }
  
      }
      et-0/0/0.0 {
        description "dut -> x1 [external]";
  
          family inet {
            address 172.16.0.1/24;
          }
  
      }
      et-0/0/1.0 {
        description "dut -> x2 [external]";
  
          family inet {
            address 172.16.1.1/24;
          }
  
      }
    }
    protocols {
      lldp {
        interface re0:mgmt-0 {
          disable;
        }
        interface all;
      }
    }

