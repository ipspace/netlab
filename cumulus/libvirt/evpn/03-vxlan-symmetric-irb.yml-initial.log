    iface vlan4000
        vrf customer
        vlan-id 4000
        vlan-raw-device bridge
    #
    CONFIG
    ifreload -a
    #
    cat >/tmp/evpn_config <<CONFIG
    router bgp 65000
     address-family l2vpn evpn
      advertise-all-vni
      advertise-svi-ip
      advertise ipv4 unicast
  
    ! Configure explicit Route Targets and RD per L2 VNI; auto-derived differs
      vni 21001
       rd 10.0.0.5:1001
       route-target export 65000:1001
       route-target import 65000:1001
      exit-vni
      vni 21000
       rd 10.0.0.5:1000
       route-target export 65000:1000
       route-target import 65000:1000
      exit-vni
  
      neighbor 10.0.0.6 activate
      neighbor 10.0.0.6 soft-reconfiguration inbound
  
     exit-address-family
    !
  
    exit
  
    ! L3 VRF EVPN handling
    vrf customer
     vni 5042
     exit-vrf
  
    router bgp 65000 vrf customer
     address-family l2vpn evpn
      ! This sets the L3 VNI RD/RT for this VRF
      rd 65000:1
      route-target export 65000:1
      route-target import 65000:1
      ! Enable RT5 advertisements
      advertise ipv4 unicast
     exit-address-family
    exit
    !
    !
    do write
    CONFIG
    vtysh -f /tmp/evpn_config
    exit $?
