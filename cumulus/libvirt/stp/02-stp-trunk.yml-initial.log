    iface bridge
     bridge-stp on
     mstpctl-forcevers stp
     #
     # Newer versions 5.x support 'mstpctl-pvrst-mode yes' to enable PVRST
     #
     mstpctl-treeprio 4096
  
    CONFIG
  
    ifreload -a
  
    #
