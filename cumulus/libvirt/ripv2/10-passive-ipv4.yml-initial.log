    #!/bin/bash
    #
    set -e
    #
    # Try to configure ripd, it will fail the first time
    #
    cat >/tmp/config <<CONFIG
    router rip
      version 2
    CONFIG
    vtysh -f /tmp/config
    #
    # Now for the real deal
    #
    cat >/tmp/config <<CONFIG
    !
    router rip
     version 2
     timers basic 5 180 120
  
     network lo
     network swp1
     network swp2
     passive-interface swp2
     network swp3
     passive-interface swp3
    !
  
    !
    do write
    CONFIG
    vtysh -f /tmp/config
    #
    # And retry for a good measure
    #
    sleep 2
    cat >/tmp/config <<CONFIG
    !
    router rip
     version 2
     timers basic 5 180 120
  
     network lo
     network swp1
     network swp2
     passive-interface swp2
     network swp3
