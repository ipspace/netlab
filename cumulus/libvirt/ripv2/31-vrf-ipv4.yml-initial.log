    #!/bin/bash
    #
    set -e # Exit immediately when any command fails
    #
    cat >/etc/network/interfaces.d/70-vrf.intf <<CONFIG
    auto blue
    iface blue
        vrf-table 1011
    #
    auto red
    iface red
        vrf-table 1010
    #
    auto swp1
    iface swp1
        vrf red
    #
    auto swp2
    iface swp2
        vrf red
    #
    auto swp3
    iface swp3
        vrf blue
    #
    auto swp4
    iface swp4
        vrf blue
    #
    CONFIG
    ifreload -a
    #
    cat >/tmp/vrf_config <<CONFIG
    vrf blue
     exit-vrf
    vrf red
     exit-vrf
    !
    router bgp 65000
    !
    !
    router bgp 65000 vrf blue
     no bgp ebgp-requires-policy
     no bgp default ipv4-unicast
     bgp router-id 10.0.0.1
     neighbor 172.16.3.5 remote-as 65101
     neighbor 172.16.3.5 description r4
     address-family ipv4 unicast
      redistribute connected
      redistribute rip
  
      label vpn export auto
      export vpn
      import vpn
      rd vpn export 65000:2
      rt vpn import 65000:2
      rt vpn export 65000:2
      neighbor 172.16.3.5 activate
      neighbor 172.16.3.5 send-community standard
      neighbor 172.16.3.5 send-community large
     exit-address-family
    !
    !
    router bgp 65000 vrf red
     no bgp ebgp-requires-policy
     no bgp default ipv4-unicast
     bgp router-id 10.0.0.1
     neighbor 172.16.1.3 remote-as 65100
     neighbor 172.16.1.3 description r2
     address-family ipv4 unicast
      redistribute connected
      redistribute rip
  
      label vpn export auto
      export vpn
      import vpn
      rd vpn export 65000:1
      rt vpn import 65000:1
      rt vpn export 65000:1
      neighbor 172.16.1.3 activate
      neighbor 172.16.1.3 send-community standard
      neighbor 172.16.1.3 send-community large
     exit-address-family
    !
    !
    router rip vrf blue
     version 2
     redistribute bgp
     redistribute connected
  
     network swp3
    !
  
    router rip vrf red
     version 2
     redistribute bgp
     redistribute connected
  
     network swp1
    !
  
    do write
    !
    CONFIG
    vtysh -f /tmp/vrf_config
    exit $?

