    #!/bin/bash
    #
    set -e # Exit immediately when any command fails
    #
    #
    # Create multi-vlan bridge, specify VLANs, and attach interfaces to it
    #
    cat >/etc/network/interfaces.d/50-bridge.intf <<CONFIG
    auto bridge
    iface bridge
        bridge-vlan-aware yes
        bridge-vids 700,701,702
        bridge-ports swp1
        bridge-ports swp2
        bridge-ports swp3
        bridge-ports swp4
    CONFIG
    #
    # Create VLAN interfaces
    #
    cat >/etc/network/interfaces.d/51-bridge-interfaces.intf <<CONFIG
    iface swp1
        bridge-access 700
    iface swp2
        bridge-access 700
    iface swp3
        bridge-access 701
    iface swp4
        bridge-access 702
    auto vlan700
    iface vlan700
  
        vlan-id 700
        vlan-raw-device bridge
    auto vlan701
    iface vlan701
        address 172.16.1.5/24
  
        vlan-id 701
        vlan-raw-device bridge
    auto vlan702
    iface vlan702
        address 172.16.2.5/24
  
        vlan-id 702
        vlan-raw-device bridge
    CONFIG
    ifreload -a

