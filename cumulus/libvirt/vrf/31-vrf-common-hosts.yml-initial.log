    #
    set -e # Exit immediately when any command fails
    #
    cat >/etc/network/interfaces.d/70-vrf.intf <<CONFIG
    auto blue
    iface blue
        vrf-table 1011
    #
    auto common
    iface common
        vrf-table 1012
    #
    auto red
    iface red
        vrf-table 1010
    #
    auto swp1
    iface swp1
        vrf red
    #
    auto swp2
    iface swp2
        vrf blue
    #
    auto swp3
    iface swp3
        vrf common
    #
    CONFIG
    ifreload -a
    #
    cat >/tmp/vrf_config <<CONFIG
    vrf blue
     exit-vrf
    vrf common
     exit-vrf
    vrf red
     exit-vrf
    !
    router bgp 65000
    !
    !
    router bgp 65000 vrf blue
     no bgp ebgp-requires-policy
     no bgp default ipv4-unicast
     bgp router-id 10.0.0.4
     address-family ipv4 unicast
      redistribute connected
  
      label vpn export auto
      export vpn
      import vpn
      rd vpn export 65000:2
      rt vpn import 65000:2 65000:3
      rt vpn export 65000:2
     exit-address-family
    !
    !
