    echo "127.0.0.1 rtr" >>/tmp/hosts
    echo "172.16.0.1 h1" >>/tmp/hosts
    echo "172.16.2.2 h2" >>/tmp/hosts
    echo "172.16.1.3 h3" >>/tmp/hosts
    echo "172.16.3.4 h4" >>/tmp/hosts
    echo "172.16.2.5 172.16.3.5 rtr-blue" >>/tmp/hosts
    echo "172.16.0.5 172.16.1.5 rtr-red" >>/tmp/hosts
    cat /etc/hosts | awk '/localhost/,/^$/' >/tmp/hosts-start
    mv /tmp/hosts-start /etc/hosts
    sort /tmp/hosts|uniq >>/etc/hosts
    #
    #
    # Disable ZTP
    #
    ztp --disable
    #
    #
    echo "INIT: creating loopback interface"
    #
