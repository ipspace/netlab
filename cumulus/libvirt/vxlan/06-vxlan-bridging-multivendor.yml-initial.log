     description s1 -> s2
     ipv6 nd ra-interval 5
     no ipv6 nd suppress-ra
    !
    interface swp2
    ! no shutdown
     description s1 -> s3
     ipv6 nd ra-interval 5
     no ipv6 nd suppress-ra
    !
    interface swp3
    ! no shutdown
     ipv6 nd ra-interval 5
     no ipv6 nd suppress-ra
    !
    interface swp4
    ! no shutdown
     ipv6 nd ra-interval 5
     no ipv6 nd suppress-ra
    !
    interface vlan1000
    ! no shutdown
     description VLAN red (1000) -> [h3,s3,h1,h2,s2]
     ipv6 nd ra-interval 5
     no ipv6 nd suppress-ra
    !
    interface vlan1001
    ! no shutdown
     description VLAN blue (1001) -> [h6,s3,h4,h5,s2]
     ipv6 nd ra-interval 5
     no ipv6 nd suppress-ra
    !
    do write
