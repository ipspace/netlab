    ip route add 10.0.0.0/24 via 172.16.6.3
    # mgmt prefix: 192.168.17.0/24 local subnet: 172.16.6.0/24
    # p2p prefix: 10.1.0.0/16 local subnet: 172.16.6.0/24
    set +e
