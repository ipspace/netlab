    interface eth1.1001
     no shutdown
     description r1 -> r2
     ip address 172.16.7.1/24
    !
    interface eth1.1000
     no shutdown
     description r1 -> r2
     ip address 172.16.8.1/24
    !
    do write
    CONFIG
    vtysh -f /tmp/config
    exit 0
