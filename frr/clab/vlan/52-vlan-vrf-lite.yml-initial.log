    # vrf_loopback prefix: 10.2.0.0/24 local subnet: 172.16.6.0/24
    set +e
    ip route del 10.2.0.0/24 2>/dev/null
    set -e
    ip route add 10.2.0.0/24 via 172.16.6.3
