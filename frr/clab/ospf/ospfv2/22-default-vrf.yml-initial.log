    #!/bin/bash
    #
    set -e # Exit immediately when any command fails
    #
  
    # Create VRF tables
    if [ ! -e /sys/devices/virtual/net/d1 ]; then
    ip link add d1 type vrf table 100
    fi
    ip link set d1 up
    if [ ! -e /sys/devices/virtual/net/d2 ]; then
    ip link add d2 type vrf table 101
    fi
    ip link set d2 up
  
    # Move interfaces and loopbacks to vrfs
    sysctl -qw net.ipv6.conf.eth1.keep_addr_on_down=1
    ip link set eth1 master d1
    sysctl -qw net.ipv6.conf.eth2.keep_addr_on_down=1
    ip link set eth2 master d2
    sysctl -qw net.ipv6.conf.eth3.keep_addr_on_down=1
    ip link set eth3 master d2
    sysctl -qw net.ipv6.conf.lo1.keep_addr_on_down=1
    ip link set lo1 master d2
  
    cat >/tmp/vrf_config <<CONFIG
    vrf d1
     exit-vrf
    vrf d2
     exit-vrf
    !
    router bgp 65000
    !
    !
    router bgp 65000 vrf d1
     no bgp ebgp-requires-policy
     no bgp default ipv4-unicast
     bgp router-id 10.0.0.1
     address-family ipv4 unicast
      redistribute connected
      redistribute ospf
  
      label vpn export auto
      export vpn
      import vpn
      rd vpn export 65000:1
      rt vpn import 65000:1
      rt vpn export 65000:1
     exit-address-family
    !
    !
    router bgp 65000 vrf d2
     no bgp ebgp-requires-policy
     no bgp default ipv4-unicast
     bgp router-id 10.0.0.1
     neighbor 10.1.0.10 remote-as 65100
     neighbor 10.1.0.10 description xf
     address-family ipv4 unicast
      redistribute connected
      redistribute ospf
  
      label vpn export auto
      export vpn
      import vpn
      rd vpn export 65000:2
      rt vpn import 65000:2
      rt vpn export 65000:2
      network 192.168.43.1/32
      neighbor 10.1.0.10 activate
      neighbor 10.1.0.10 send-community standard
      neighbor 10.1.0.10 send-community large
     exit-address-family
    !
    !
    !
    ! OSPFv2 FRR configuration
    !
    router ospf vrf d1
     ospf router-id 10.0.0.1
     timers throttle spf 10 50 500
     timers throttle lsa all 100
     timers lsa min-arrival 100
     default-information originate always
  
     redistribute bgp
     redistribute connected
  
    exit
    !
    interface eth1
    ! dut -> p1
     ip ospf area 0.0.0.0
     ip ospf network point-to-point
    !
  
    !
    !
    ! OSPFv2 FRR configuration
    !
    router ospf vrf d2
     ospf router-id 10.0.0.1
     timers throttle spf 10 50 500
     timers throttle lsa all 100
     timers lsa min-arrival 100
     default-information originate
  
     redistribute bgp
     redistribute connected
  
    exit
    !
    interface eth2
    ! dut -> p2
     ip ospf area 0.0.0.0
     ip ospf network point-to-point
    !
    interface lo1
    ! VRF Loopback d2
     ip ospf area 0.0.0.0
    !
  
    !
    do write
    !
    CONFIG
    vtysh -f /tmp/vrf_config
    exit $?

